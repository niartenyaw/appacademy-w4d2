<h1>Cat Rentals</h1>
<table>
  <tr>
    <th>Cat ID</th>
    <th>Start Date</th>
    <th>End Date</th>
    <!-- <th>Status</th> -->
  </tr>
  <% @requests.each do |request| %>
  <tr>
    <td> <%= request.cat_id %> </td>
    <td> <%= request.start_date %> </td>
    <td> <%= request.end_date %> </td>
    <!-- <td> <%#= request.status %> </td> -->
  </tr>
  <% end %>
</table>
